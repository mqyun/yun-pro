.maxLoadingTip
  position fixed
  top 0
  left 0
  width 100%
  height 100%

.wrapLoadingTip
  position absolute
  top 0
  left 0
  width 100%
  height 100%

.maxLoadingTip img,
.wrapLoadingTip img
  width 42px
  height 42px
  position absolute
  top 50%
  left 50%
  margin-left -21px
  margin-top -21px